{
  "name": "termina-sol",
  "version": "0.1.0",
  "description": "Personal website and app.",
  "author": "Termina Sunshine <katvance64@gmail.com>",
  "license": "GPL-3.0-or-later",
  "private": false,
  "type": "module",
  "cfg": {
    "dist_dir": "./dist/",
    "webdir": "./packages/client-web/",
    "nativedir": "./packages/client-native/"
  },
  "scripts": {
    "webpack": "webpack",
    "install:web": "cd $npm_package_cfg_webdir && yarn install",
    "install:native": "cd $npm_package_cfg_nativedir && yarn install",
    "build:web": "yarn run webpack",
    "build:native": "cd $npm_package_cfg_nativedir && yarn run dev",
    "open": "yarn run build:web --mode development; cd $npm_package_cfg_nativedir/src-tauri && yarn run dev",
    "serve": "webpack serve --mode=development",
    "start": "webpack serve --open --mode=development",
    "dist": "rm -rf ./dist/ && yarn run build:web && webpack --mode=production",
    "postinstall": "yarn run install:web; yarn run install:native"
  },
  "dependencies": {
    "webpack": "^5.67.0",
    "webpack-cli": "^4.9.1"
  },
  "devDependencies": {
    "css-loader": "^6.6.0",
    "favicons": "^6.2.1",
    "favicons-webpack-plugin": "^5.0.2",
    "file-loader": "^6.2.0",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "remark": "^13.0.0",
    "remark-html": "^15.0.1",
    "remark-loader": "^4.0.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
    "url-loader": "^4.1.1",
    "webpack-dev-server": "^4.7.3"
  }
}
